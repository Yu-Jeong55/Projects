{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "data": ["src/data/*"],
      "assets": ["src/assets/*"],
      "components": ["src/components/*"],
      "hooks": ["src/hooks/*"],
      "pages": ["src/pages/*"],
      "state": ["src/state/*"],
      "styles": ["src/styles/*"]
    }
  },
  "include": ["src", "**/*.js", "**/*.jsx"],
  "exclude": ["node_modules"]
}
